{
  "kind": "collectionType",
  "collectionName": "locations",
  "info": {
    "singularName": "location",
    "pluralName": "locations",
    "displayName": "location",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "lon": {
      "type": "decimal"
    },
    "lat": {
      "type": "decimal"
    },
    "name": {
      "type": "string"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "blocks"
    },
    "user_map_progresses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::user-map-progress.user-map-progress",
      "mappedBy": "visited_locations"
    },
    "user_map_progress": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-map-progress.user-map-progress",
      "mappedBy": "last_location"
    },
    "game": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::game.game",
      "mappedBy": "location"
    },
    "finish": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::finish.finish",
      "mappedBy": "location"
    },
    "user_location_progresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-location-progress.user-location-progress",
      "mappedBy": "location"
    }
  }
